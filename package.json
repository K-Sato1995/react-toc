  "license": "MIT",
  "repository": "K-Sato1995/react-toc",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "scripts": {
    "test": "./node_modules/.bin/jest --config ./jest.config.js",
    "lint": "eslint --ext=ts,tsx src",
