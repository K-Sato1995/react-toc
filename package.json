    "react-test-renderer": "^16.13.1",
    "ts-jest": "^26.1.1"
  },
  "peerDependencies": {
    "prop-types": "^15.5.4",
    "react": "^15.0.0 || ^16.0.0",
    "react-dom": "^15.0.0 || ^16.0.0"
  },
  "devDependencies": {
    "@svgr/rollup": "^5.4.0",
    "@types/jest": "^26.0.4",
    "@types/react": "^16.3.13",
    "@types/react-dom": "^16.0.5",
    "babel-core": "^6.26.3",
    "babel-runtime": "^6.26.0",
    "cross-env": "^7.0.2",
    "gh-pages": "^3.1.0",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-scripts-ts": "^3.1.0",
    "rollup": "^2.21.0",
    "rollup-plugin-babel": "^4.4.0",
