{
  "name": "react-toc",
  "version": "1.2.0",
  "description": "Create a table of contents from the given markdown text.",
  "author": "K-Sato1995",
  "license": "MIT",
  "repository": "K-Sato1995/react-toc",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "scripts": {
    "test": "yarn jest",
    "test:watch": "react-scripts-ts test --env=jsdom",
